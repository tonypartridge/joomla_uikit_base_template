<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE install PUBLIC "-//Joomla! 2.5//DTD template 1.0//EN" "https://www.joomla.org/xml/dtd/2.5/template-install.dtd">
<extension version="3.1" type="template" client="site">
    <name>Simple UIKit</name>
    <version>1.0</version>
    <creationDate>20/05/2017</creationDate>
    <author>Tony Partridge</author>
    <authorEmail>admin@joomla.org</authorEmail>
    <copyright>Copyright (C) 2005 - 2017 Open Source Matters, Inc. All rights reserved.</copyright>
    <description>Basic template using UIKit2. Positions need to be set.</description>
    <files>
        <filename>component.php</filename>
        <filename>error.php</filename>
        <filename>offline.php</filename>
        <filename>favicon.ico</filename>
        <filename>index.php</filename>
        <filename>templateDetails.xml</filename>
        <filename>template_preview.png</filename>
        <filename>template_thumbnail.png</filename>
        <filename>bower.json</filename>
        <folder>css</folder>
        <folder>html</folder>
        <folder>images</folder>
        <folder>img</folder>
        <folder>js</folder>
        <folder>language</folder>
        <folder>less</folder>
        <folder>scss</folder>
    </files>
    <positions>
        <position>debug</position>
    </positions>
    <config>
        <fields name="params">
            <fieldset name="advanced">
            </fieldset>
        </fields>
    </config>
</extension>
